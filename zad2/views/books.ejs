<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/navigation') %>
    <title><%= pageTitle %></title>
</head>
<body>
    <h1>Books</h1>
    <ul>
        <% if (books) { %>
            <% books.forEach(function(book) { %>
                <li><%= book.title %></li>
            <% }); %>
        <% } else { %>
            <li>No books found</li>
        <% } %>
    </ul>
</body>
</html>
