<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head', { pageTitle: 'Authors' }) %>
</head>
<body>
    <%- include('partials/navigation') %>
    <h1>Authors</h1>
    <ul>
        <% if (authors) { %>
            <% authors.forEach(function(author) { %>
                <li><%= author.name %></li>
            <% }); %>
        <% } else { %>
            <li>No authors found</li>
        <% } %>
    </ul>
</body>
</html>
